      "gmd:title": {
       "@xsi:type": "gmd:PT_FreeText_PropertyType",
       "gco:CharacterString": "M-POETE (Mobile Platform for the Observation and the Experimentation in Terrestrial Ecosystems)",
       "gmd:PT_FreeText": {
        "gmd:textGroup": {
         "gmd:LocalisedCharacterString": {
          "@locale": "#FR",
          "#text": "M-POETE (Mobile Platform for the Observation and the Experimentation in Terrestrial Ecosystems)"
         }
        }
