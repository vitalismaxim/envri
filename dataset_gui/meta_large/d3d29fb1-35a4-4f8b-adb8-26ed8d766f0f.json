     "gmd:CI_Citation": {
      "gmd:title": {
       "@xsi:type": "gmd:PT_FreeText_PropertyType",
       "gco:CharacterString": "Platform GenoSol : a logistical and technical tool for microbial diversity research",
       "gmd:PT_FreeText": {
        "gmd:textGroup": {
         "gmd:LocalisedCharacterString": {
          "@locale": "#FR",
          "#text": "Plateforme GenoSol : Une structure logistique et technique d'appui d\u00c3\u00a9di\u00c3\u00a9e aux recherches en \u00c3\u00a9cologie microbienne"
         }
        }
       }
      },
